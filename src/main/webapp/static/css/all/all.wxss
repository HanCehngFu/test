}
.show {
  display: block;
}
.hide {
  display: none;
}
p {
  margin-bottom: 0;
}
h3 {
  margin-bottom: 0;
  font-weight: 700;
}
h4 {
  font-size: 20px;
  font-weight: 700;
  color: #000;
}
i {
  font-style: normal;
}
input:focus {
  border: 1px solid rgba(0, 0, 0, 0.1490196);
}
::-webkit-scrollbar {
  display: none;
}
::-webkit-input-placeholder {
  /* WebKit browsers */
  color: rgba(0, 0, 0, 0.25);
}
::-moz-placeholder {
  /* Mozilla Firefox 19+ */
