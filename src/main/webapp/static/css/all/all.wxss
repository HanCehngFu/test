/* 清除浮动 */
.clearfix:after {
  content: '';
  display: block;
  height: 0;
  clear: both;
  font-size: 0;
  visibility: hidden;
}
* {
  box-sizing: border-box;
}
ul,
li {
  list-style: none;
  padding: 0;
  margin: 0;
}
a {
  color: rgba(0, 0, 0, 0.65);
}
a:hover {
  color: rgba(0, 0, 0, 0.65);
}
